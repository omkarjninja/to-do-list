<%- include("header")-%>
  <div class="box" id="heading">
    <h1 class="date" style="color: black;"><%= listTitle %></h1>
  </div>
   <div class="box">
    
      <% for(var i=0;i< newtaskitem.length;i++) {%>
        <div class="item">
          <input type="checkbox">
        <p><%= newtaskitem[i] %></p>
      </div>
      <% } %>
      <form class="item" action="/" method="post">
        <input type="text" name="newtask" placeholder="New Task" autocomplete="off">
        <br>
        <button type="submit" name="type" value="<%= listTitle %>">+</button>
      </form>
    </div>
    

    <%- include("footer")-%>